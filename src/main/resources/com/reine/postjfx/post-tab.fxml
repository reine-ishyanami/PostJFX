         xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.6">
    <tabs>
        <Tab closable="false" disable="true" fx:id="btnTab">
            <graphic>
                <Button fx:id="addTabButton" onAction="#addPostPage" text="+" focusTraversable="false"/>
            </graphic>
        </Tab>
    </tabs>
</fx:root>
