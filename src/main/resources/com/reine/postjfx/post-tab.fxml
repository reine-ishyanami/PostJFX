<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<fx:root type="TabPane" stylesheets="@css/post-tab.css"
         xmlns="http://javafx.com/javafx/21.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <tabs>
        <Tab closable="false" disable="true" fx:id="btnTab">
            <graphic>
                <Button fx:id="addTabButton" onAction="#addPostPage" focusTraversable="false"/>
            </graphic>
        </Tab>
    </tabs>
</fx:root>
